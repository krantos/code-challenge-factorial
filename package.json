{
  "name": "code-challenge-factorial",
  "version": "1.0.0",
  "description": "Automation code challenge",
  "main": "index.js",
  "scripts": {
    "test": "test",
    "cypress:open": "cypress open",
    "report:merge": "mochawesome-merge ./cypress/result/json/*.json > ./cypress/result/mochawesome-bundle.json",
    "report:generate": "marge ./cypress/result/mochawesome-bundle.json -o cypress/report/index -i & npm run report:copyScreenshots",
    "report:copyScreenshots": "cp -r cypress/screenshots cypress/report/",
    "report": "node cypress/cucumber-json/html-report-generator.js",
    "report:clean": "rm cypress/cucumber-json/*.json & rm -r cypress/cucumber-json/report & rm -r cypress/result & rm -r cypress/report & rm -r cypress/screenshots"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krantos/code-challenge-factorial.git"
  },
  "author": "Marcos Riveros",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/krantos/code-challenge-factorial/issues"
  },
  "homepage": "https://github.com/krantos/code-challenge-factorial#readme",
  "devDependencies": {
    "@testing-library/cypress": "^7.0.3",
    "cypress": "^6.1.0",
    "cypress-cucumber-preprocessor": "^4.0.0",
    "cypress-multi-reporters": "^1.4.0",
    "eslint": "^7.15.0",
    "eslint-plugin-cypress": "^2.11.2",
    "mocha": "^8.2.1",
    "mochawesome": "^6.2.1",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.1.0",
    "multiple-cucumber-html-reporter": "^1.18.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "dependencies": {}
}
